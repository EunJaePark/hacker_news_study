TCH_COMMENT(context, commId) {\n      fetchCommentItem(commId)\n      .then(res => {\n        context.commit('SET_COMMENT', res.data)\n      })\n      .catch(err => {\n        console.log(err);\n        })\n    },\n\n    // 작성자\n    FETCH_USER(context, userName) {\n      return fetchCommentItem(userName)\n      .then(res => {\n        context.commit('SET_USER', res.data)\n      })\n      .catch(err => {\n        console.log(err);\n      })\n    }\n    \n\n\n\n\n\n    // FETCH_Comment(context, commentId) {\n    //   // return fetchAskItem(askId).then().catch()\n    //   return fetchCommentItem(commentId)\n    //   .then(res => {\n    //     context.commit('SET_COMMENT', res.data);\n    //     return res;\n    //   })\n    //   .catch(err => {\n    //     console.log(err);\n        \n    //   })\n    // }\n  },\n  modules: {\n  }\n})\n"]}]}